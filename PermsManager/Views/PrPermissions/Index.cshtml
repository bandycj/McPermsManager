@model IEnumerable<PermsManager.Models.PrPermission>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            permstring
        </th>
        <th>
            PrEntry
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.permstring)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrEntry.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.permid }) |
            @Html.ActionLink("Details", "Details", new { id=item.permid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.permid })
        </td>
    </tr>
}

</table>
